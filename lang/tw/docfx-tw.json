{
   "metadata":[
      {
         "src":[
            {
               "files":[
                  "src/**.csproj"
               ]
            }
         ],
         "dest":"api",
         "disableGitFeatures":false,
         "disableDefaultFilter":false
      }
   ],
   "build":{
      "content":[
         {
            "files":[
               "products/dmflow/**.yml",
               "products/dmflow/**.md"
            ]
         },
         {
            "files":[
               "toc.yml",
               "*.md"
            ]
         }
      ],
      "resource":[
         {
            "files":[
               "images/**"
            ]
         }
      ],
      "overwrite":[
         {
            "files":[
               "apidoc/**.md"
            ],
            "exclude":[
               "obj/**",
               "_site/**"
            ]
         }
      ],
      "dest":"../../docs/tw",
      "globalMetadataFiles":[
         "global.json"
      ],
      "fileMetadataFiles":[
         
      ],
      "template":[
         "default",
         "templates/material"
      ],
      "postProcessors":[
         
      ],
      "markdownEngineName":"markdig",
      "noLangKeyword":false,
      "keepFileLink":false,
      "cleanupCacheHistory":false,
      "disableGitFeatures":false
   },
   "pdf":{
      "content":[
         {
            "files":[
               "api/**.yml",
               "api-vb/**.yml"
            ],
            "exclude":[
               "**/toc.yml",
               "**/toc.md"
            ]
         },
         {
            "files":[
               "products/dmflow/**.yml",
               "products/dmflow/**.md",
               "pdf/*"
            ],
            "exclude":[
               "**/bin/**",
               "**/obj/**",
               "_site_pdf/**",
               "**/toc.yml",
               "**/toc.md"
            ]
         },
         {
            "files":"pdf/toc.yml"
         }
      ],
      "resource":[
         {
            "files":[
               "images/**"
            ],
            "exclude":[
               "**/bin/**",
               "**/obj/**",
               "_site_pdf/**"
            ]
         }
      ],
      "overwrite":[
         {
            "files":[
               "apidoc/**.md"
            ],
            "exclude":[
               "**/bin/**",
               "**/obj/**",
               "_site_pdf/**"
            ]
         }
      ],
      "wkhtmltopdf":{
         "additionalArguments":"--enable-local-file-access"
      },
      "dest":"_site_pdf"
   }
}