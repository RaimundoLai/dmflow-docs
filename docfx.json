{
   "metadata":[
      {
         "src":[
            {
               "files":[
                  "src/**.csproj"
               ]
            }
         ],
         "dest":"api",
         "disableGitFeatures":false,
         "disableDefaultFilter":false
      }
   ],
   "build":{
      "content":[
         {
            "files":[
               "products/dmflow/**.yml",
               "products/dmflow/**.md"
            ]
         },
         {
            "files":[
               "toc.yml",
               "*.md"
            ]
         }
      ],
      "resource":[
         {
            "files":[
               "images/**",
               "googled3dd506f4820cbd9.html",
               "CNAME",
			   "files/**",
			   "robots.txt"
            ]
         }
      ],
      "overwrite":[
         {
            "files":[
               "apidoc/**.md"
            ],
            "exclude":[
               "obj/**",
               "_site/**"
            ]
         }
      ],
      "dest":"docs",
      "globalMetadataFiles":[
         "global.json"
      ],
      "fileMetadataFiles":[
         
      ],
      "template":[
         "default",
         "templates/material",
		 "templates/mermaid"
      ],
      "postProcessors":[
         
      ],
      "markdownEngineName":"markdig",
      "noLangKeyword":false,
      "keepFileLink":false,
      "cleanupCacheHistory":false,
      "disableGitFeatures":false,
		"sitemap": {
			"baseUrl": "https://docs-on-prem.dmflow.chat",
			"changefreq": "weekly",
			"priority": 1.0
		}
   },
   "pdf":{
      "content":[
         {
            "files":[
               "lang/tw/products/dmflow/**.yml",
               "lang/tw/products/dmflow/**.md",
			   "lang/en/products/dmflow/**.yml",
               "lang/en/products/dmflow/**.md",
               "pdf/tw/*",
               "pdf/en/*"
            ],
            "exclude":[
               "**/bin/**",
               "**/obj/**",
               "_site_pdf/**",
               "**/toc.yml",
               "**/toc.md"
            ]
         },
         {
            "files":"pdf/tw/toc.yml"
         },
         {
            "files":"pdf/en/toc.yml"
         }
      ],
      "resource":[
         {
            "files":[
               "images/**"
            ],
            "exclude":[
               "**/bin/**",
               "**/obj/**",
               "_site_pdf/**"
            ]
         }
      ],
      "overwrite":[
         {
            "files":[
               "apidoc/**.md"
            ],
            "exclude":[
               "**/bin/**",
               "**/obj/**",
               "_site_pdf/**"
            ]
         }
      ],
      "wkhtmltopdf":{
         "additionalArguments":"--enable-local-file-access"
      },
      "dest":"_site_pdf"
   }
}